<?xml version="1.0" ?>
<robot name = "cup">

  <link name="base">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx = "0.0414613" ixy = "0" ixz = "0" iyx = "0" iyy = "0.0956574" iyz = "0" izx = "0" izy = "0" izz = "0.121322"/>
    </inertial>
