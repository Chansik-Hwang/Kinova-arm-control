    </inertial>
    <visual>
      <origin rpy="1.57 0 0" xyz="0 0 0"/>
      <geometry>
           <mesh filename="../meshes/cup.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="pink">
      <color rgba="1 0.76862745098 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
	 	 <mesh filename="../meshes/cup.obj" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="1.57 0 0" xyz="0 0 0"/>
    </collision>
  </link>


